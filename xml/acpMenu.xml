<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="http://www.woltlab.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.woltlab.com http://www.woltlab.com/XSD/maelstrom/acpMenu.xsd">
	<import>
		<acpmenuitem name="fireball.acp.menu.link.cms">
			<showorder>6</showorder>
		</acpmenuitem>

		<!-- page -->
		<acpmenuitem name="fireball.acp.menu.link.fireball.page">
			<parent>fireball.acp.menu.link.cms</parent>
		</acpmenuitem>
		<acpmenuitem name="fireball.acp.menu.link.fireball.page.list">
			<parent>fireball.acp.menu.link.fireball.page</parent>
			<controller>cms\acp\page\PageListPage</controller>
			<permissions>admin.fireball.page.canListPage</permissions>
		</acpmenuitem>
		<acpmenuitem name="fireball.acp.menu.link.fireball.page.add">
			<parent>fireball.acp.menu.link.fireball.page</parent>
			<controller>cms\acp\form\PageAddForm</controller>
			<permissions>admin.fireball.page.canAddPage</permissions>
		</acpmenuitem>
		<acpmenuitem name="fireball.acp.menu.link.fireball.page.statistics">
			<parent>fireball.acp.menu.link.fireball.page</parent>
			<controller>cms\acp\page\StatsPage</controller>
			<options>cms_pages_enable_statistics</options>
		</acpmenuitem>
		<acpmenuitem name="fireball.acp.menu.link.fireball.page.importAndExport">
			<parent>fireball.acp.menu.link.fireball.page</parent>
			<controller>cms\acp\form\CMSImportForm</controller>
			<permissions>admin.fireball.page.canAddPage</permissions>
		</acpmenuitem>
		<!-- /page -->

		<!-- file -->
		<acpmenuitem name="fireball.acp.menu.link.fireball.file">
			<parent>fireball.acp.menu.link.cms</parent>
		</acpmenuitem>
		<acpmenuitem name="fireball.acp.menu.link.fireball.file.list">
			<parent>fireball.acp.menu.link.fireball.file</parent>
			<controller>cms\acp\page\FileListPage</controller>
			<permissions>admin.fireball.file.canAddFile</permissions>
		</acpmenuitem>
		<acpmenuitem name="fireball.acp.menu.link.fireball.file.category.list">
			<parent>fireball.acp.menu.link.fireball.file</parent>
			<controller>cms\acp\page\FileCategoryListPage</controller>
			<permissions>admin.fireball.file.canAddFile</permissions>
		</acpmenuitem>
		<acpmenuitem name="fireball.acp.menu.link.fireball.file.category.add">
			<parent>fireball.acp.menu.link.fireball.file</parent>
			<controller>cms\acp\form\FileCategoryAddForm</controller>
			<permissions>admin.fireball.file.canAddFile</permissions>
		</acpmenuitem>
		<!-- /file -->

		<!-- stylesheet -->
		<acpmenuitem name="fireball.acp.menu.link.fireball.stylesheet">
			<parent>fireball.acp.menu.link.cms</parent>
		</acpmenuitem>
		<acpmenuitem name="fireball.acp.menu.link.fireball.stylesheet.list">
			<parent>fireball.acp.menu.link.fireball.stylesheet</parent>
			<controller>cms\acp\page\StylesheetListPage</controller>
			<permissions>admin.fireball.style.canListStylesheet</permissions>
		</acpmenuitem>
		<acpmenuitem name="fireball.acp.menu.link.fireball.stylesheet.add">
			<parent>fireball.acp.menu.link.fireball.stylesheet</parent>
			<controller>cms\acp\form\StylesheetAddForm</controller>
			<permissions>admin.fireball.style.canAddStylesheet</permissions>
		</acpmenuitem>
		<!-- /stylesheet -->
	</import>
</data>
