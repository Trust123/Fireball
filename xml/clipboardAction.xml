<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="http://www.woltlab.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.woltlab.com http://www.woltlab.com/XSD/maelstrom/clipboardAction.xsd">
	<import>
		<!-- content -->
		<action name="delete">
			<actionclassname>cms\system\clipboard\action\ContentClipboardAction</actionclassname>
			<showorder>1</showorder>
			<pages>
				<page>cms\acp\page\ContentListPage</page>
			</pages>
		</action>
		<action name="enable">
			<actionclassname>cms\system\clipboard\action\ContentClipboardAction</actionclassname>
			<showorder>2</showorder>
			<pages>
				<page>cms\acp\page\ContentListPage</page>
			</pages>
		</action>
		<action name="disable">
			<actionclassname>cms\system\clipboard\action\ContentClipboardAction</actionclassname>
			<showorder>3</showorder>
			<pages>
				<page>cms\acp\page\ContentListPage</page>
			</pages>
		</action>
		<action name="copyTo">
			<actionclassname>cms\system\clipboard\action\ContentClipboardAction</actionclassname>
			<showorder>4</showorder>
			<pages>
				<page>cms\acp\page\ContentListPage</page>
			</pages>
		</action>
		<action name="moveTo">
			<actionclassname>cms\system\clipboard\action\ContentClipboardAction</actionclassname>
			<showorder>5</showorder>
			<pages>
				<page>cms\acp\page\ContentListPage</page>
			</pages>
		</action>
		<!-- /content -->

		<!-- file -->
		<action name="delete">
			<actionclassname>cms\system\clipboard\action\FileClipboardAction</actionclassname>
			<showorder>1</showorder>
			<pages>
				<page>cms\acp\page\FileListPage</page>
			</pages>
		</action>
		<!-- /file -->

		<!-- page -->
		<action name="delete">
			<actionclassname>cms\system\clipboard\action\PageClipboardAction</actionclassname>
			<showorder>1</showorder>
			<pages>
				<page>cms\acp\page\PageListPage</page>
			</pages>
		</action>
		<action name="enable">
			<actionclassname>cms\system\clipboard\action\PageClipboardAction</actionclassname>
			<showorder>2</showorder>
			<pages>
				<page>cms\acp\page\PageListPage</page>
			</pages>
		</action>
		<action name="disable">
			<actionclassname>cms\system\clipboard\action\PageClipboardAction</actionclassname>
			<showorder>3</showorder>
			<pages>
				<page>cms\acp\page\PageListPage</page>
			</pages>
		</action>
		<!-- /page -->

		<!-- stylesheet -->
		<action name="delete">
			<actionclassname>cms\system\clipboard\action\StylesheetClipboardAction</actionclassname>
			<showorder>1</showorder>
			<pages>
				<page>cms\acp\page\StylesheetListPage</page>
			</pages>
		</action>
		<!-- /stylesheet -->
	</import>
</data>
