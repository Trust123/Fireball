<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="http://www.woltlab.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.woltlab.com http://www.woltlab.com/XSD/maelstrom/bbcode.xsd">
	<import>
		<bbcode name="cmsfile">
			<classname><![CDATA[cms\system\bbcode\CMSFileBBCode]]></classname>
			<allowedchildren><![CDATA[none^b,i,u,s,sub,sup,color,font,url]]></allowedchildren>
			<attributes>
				<attribute name="0">
					<validationpattern><![CDATA[^\d+$]]></validationpattern>
					<required>1</required>
					<usetext>1</usetext>
				</attribute>
				<attribute name="1">
					<validationpattern>^(left|right|none)$</validationpattern>
					<required>0</required>
				</attribute>
				<attribute name="2">
					<validationpattern><![CDATA[^\d+$]]></validationpattern>
					<required>0</required>
				</attribute>
				<attribute name="3">
					<usetext>1</usetext>
				</attribute>
			</attributes>
		</bbcode>
	</import>
	<import>
		<bbcode name="fireball">
			<classname><![CDATA[cms\system\bbcode\PageBBCode]]></classname>
			<allowedchildren><![CDATA[none]]></allowedchildren>
			<attributes>
				<attribute name="0">
					<validationpattern><![CDATA[^\d+$]]></validationpattern>
					<required>1</required>
					<usetext>1</usetext>
				</attribute>
			</attributes>
			<wysiwyg>1</wysiwyg>
			<wysiwygicon>fa-file-text-o</wysiwygicon>
			<buttonlabel>cms.bbcode.button.page</buttonlabel>
		</bbcode>
	</import>
	<import>
		<bbcode name="cmsfileurl">
			<classname><![CDATA[cms\system\bbcode\CMSFileUrlBBCode]]></classname>
			<allowedchildren><![CDATA[none]]></allowedchildren>
		</bbcode>
	</import>
</data>
